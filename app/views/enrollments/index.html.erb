<p id="notice"><%= notice %></p>

<h1>Listing Enrollments</h1>

<table class = "table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Course code</th>
      <th>Course name</th>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enrollments.each do |enrollment| %>
      <tr>
        <td><%= enrollment.email %></td>
        <td><%= enrollment.course_code %></td>
        <td><%= enrollment.course_name %></td>
        <td><%= enrollment.subject_id %></td>
        <td><%= link_to 'Show', enrollment %></td>
        <td><%= link_to 'Edit', edit_enrollment_path(enrollment) %></td>
        <td><%= link_to 'Destroy', enrollment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
