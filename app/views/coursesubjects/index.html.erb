<p id="notice"><%= notice %></p>

<h1>Listing Coursesubjects</h1>

<table>
  <thead>
    <tr>
      <th>Course code</th>
      <th>Course name</th>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coursesubjects.each do |coursesubject| %>
      <tr>
        <td><%= coursesubject.course_code %></td>
        <td><%= coursesubject.course_name %></td>
        <td><%= coursesubject.subject_id %></td>
        <td><%= link_to 'Show', coursesubject %></td>
        <td><%= link_to 'Edit', edit_coursesubject_path(coursesubject) %></td>
        <td><%= link_to 'Destroy', coursesubject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coursesubject', new_coursesubject_path %>
